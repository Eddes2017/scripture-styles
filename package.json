{
  "name": "api-bible-assets",
  "version": "0.1.0",
  "description": "SASS and CSS styles for API.Bible Scripture Output",
  "main": "index.js",
  "style": "css/eb-scripture-style.css",
  "sass": "scss/eb-scripture-style/eb-scripture-style.scss",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npm run build-nested && npm run build-compressed && npm run gzip",
    "build-nested": "node-sass scss/eb-scripture-style/eb-scripture-style.scss css/eb-scripture-style.css --source-map true --output-style nested",
    "build-compressed": "node-sass scss/eb-scripture-style/eb-scripture-style.scss css/eb-scripture-style.min.css --source-map true --output-style compressed",
    "gzip": "gzip < css/eb-scripture-style.min.css > css/eb-scripture-style.min.css.gz"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/americanbible/api-bible-assets.git"
  },
  "author": "American Bible Society",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/americanbible/api-bible-assets/issues"
  },
  "homepage": "https://github.com/americanbible/api-bible-assets#readme",
  "dependencies": {
    "node-sass": "^4.11.0"
  }
}
